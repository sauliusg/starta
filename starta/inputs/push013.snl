use * from std;

use APush (int);
use APrint (int);

var m = new int[0];

. "length(m) ==", length(m);
. "nref(m)   ==", nref(m);
. m;

. "========================================";

m = APush::push( m, 10 );
m = APush::push( m, 12 );

. "length(m) ==", length(m);
. "nref(m)   ==", nref(m);
. m;
