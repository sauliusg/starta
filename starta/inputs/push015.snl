//--*- Java -*--
// Compiler tests
//

use * from std;

use APush;
use APrint (string);

var m = new string[0];

. "length(m) ==", length(m);
. "nref(m)   ==", nref(m);
. m;

. "========================================";

m = APush::push( m, "delta" );

. "length(m) ==", length(m);
. "nref(m)   ==", nref(m);
. m;

. "========================================";

m = APush::push( m, "epsilon" );

. "length(m) ==", length(m);
. "nref(m)   ==", nref(m);
. m;
