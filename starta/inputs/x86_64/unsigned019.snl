#
# Snail compiler tests
#
#
# Test uncheched unsigned int conversions.
#

use std;
use unsigned;

#------------------------------------------------------------------------------

var ub : ubyte;

ub = atoub( "12" );;

. ub;

ub = atoub( "255" );

. ub;

ub = strtoub( "255" );

. ub;

#------------------------------------------------------------------------------

var uh : ushort;

uh = atouh( "12" );;

. uh;

uh = atouh( "65535" );

. uh;

uh = strtouh( "65535" );

. uh;

#------------------------------------------------------------------------------

var u : uint;

u = atoui( "12" );;

. u;

u = atoui( "4294967295" );

. u;

u = strtoui( "4294967295" );

. u;

#------------------------------------------------------------------------------

var ul : ulong;

ul = atoul( "12" );;

. ul;

ul = atoul( "4294967295" );

. ul;

ul = strtoul( "4294967295" );

. ul;

#------------------------------------------------------------------------------

var ull : ullong;

ull = atoull( "12" );;

. ull;

ull = atoull( "18446744073709551615" );

. ull;

ull = strtoull( "18446744073709551615" );

. ull;
