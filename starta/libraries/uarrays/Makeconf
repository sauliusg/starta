#--*- Makefile -*--

include ../../Makeconf

CWD = $(shell pwd -L)

TOP1  = $(dir ${CWD})
TOP2 = $(dir ${TOP1:%/=%})
TOP3 = $(dir ${TOP2:%/=%})

CFLAGS = -I. -I${TOP2} -I${TOP3} -I${TOP2}${INTERPRETER_DIR} -I${TOP3}cexceptions

LIBFLAGS = \
${TOP2}libstarta.so \
${TOP3}cexceptions/libcexceptions.a \
#
