#
# Snail compiler tests
#
#
# Test compatibility of synonims with overriden operators.
#

use * from std;

type cm = like float {
    inline bytecode operator "/" ( c1, c2 : cm ) : float
        {
          SLDC "This is the cm \"/\" operator\n"
          SPRINT
          FDIV
        }
};

var c1, c2 : cm;

c1 = 5 cm;
c2 = 2 cm;

var float x = c1/c2;
. x;

. c1 + c2, "cm";

var a, b : cm = 1.1, 2.2;

. a + b;

. a + x;
. a / x;
. a / b;
