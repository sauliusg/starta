
use std;

program mano( string argv[]; file stdio[]; string env[] )
begin
    if length(argv) > 1 then
        . argv[0] _ ":", "Hello,", argv[1]
    else
        . argv[0] _ ":", "Hello Kitty!"
    endif

    . "Our environment is:";
    for var i = 0 to last(env) do
        <stdio[1]> << env[i] << "\n";
    enddo
end;
