//--*- Java -*--
// Compiler tests
//

use * from std;

type cell = int;
type voxel = float;
type pixel = float;

// Types 'voxel' and 'pixel' should be identical to 'float'

var x, y, z : float;
var v1, v2, v3 : voxel;
var p1, p2, p3 : pixel = 1.1, 2.2, 3.3;

v1 = v2 = 10.;
v3 = 1.13E1;

x = v1;
v1 = p3 = y;

var m = new voxel[100];
var n : array of pixel;

m = [ 20.7, 30.1, 40.11 ];

n = m;

z = m[0] + v2 + p3 + x;

z += n[0] + v2 + p3 + x;

. n[1];
. "%3.4f" %% z;
