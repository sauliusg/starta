#
# Test C-style variable declarators
#

include "stdtypes.slib"
include "stdio.slib"

struct S {
    var int i;
    var int[][] m;
    var int[] n[];
    var int p[][], q[][];
}

var S s;

s = new S;

s.m = new int[10][20];
s.n = new int[3][3];
s.p = new int[5][5];
s.q = new int[6][6];

s.i = 20;

s.m[1][1] = s.i;

. s.m[1][1];
. s.n[1][1];
. s.p[1][1];
. s.q[1][1];
