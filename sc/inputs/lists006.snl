#
# Regression test:
#

# Check that lists of reference variables are looped correctly:

from std use *;
from Lists use *;

class S
{ 
  int x

  constructor S( x : int = 111 )
  { self.x = x }
}

var a : list of S;

foreach var e in a
do
    . e.x
enddo

a = cons( new S, cons( new S(), cons( new S(3) )));

foreach var e in a
do
    . e.x
enddo
