#
# Snail compiler tests
#
#
# Test how the SL compiler deals with the immutable values when
# compiling the '=' assignment.
#

use std;

readonly int x = 20;

var nn = [ 1, 2, x, 4 ];

nn[3] = x;

var int i;

for i = 0 to last(nn) do
    . nn[i];
enddo;

. "";

nn = [ 1, 2, 4, x ];

for i = 0 to last(nn) do
    . nn[i];
enddo;

. "";

nn = [ x, 2, 4 ];

for i = 0 to last(nn) do
    . nn[i];
enddo;
