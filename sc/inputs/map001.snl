#
# Test 'map' function
#

use std;

procedure map( p: function (x:type T)->(type T); m: array of type T ): 
    array of type T
{
    for var i = 0L to llast(m) do
        m[i] = p(m[i]);
    enddo

    return m;
}

var m = [1,2,3];

var n = map( function (i:int)->(int) { return i+1 }, m );

foreach var k in n do
    . k;
enddo
