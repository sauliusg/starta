// An example of a module parametrised with a constant, a variable and
// a procedure.

module ProcTypeParam( const C, type T, var X, function P )

use * from std;

var inner: T;

procedure pp( T x ): T
{
    var T c = const(C) T;
    var T s = c + X;
    return P(s, x);
}

end module ProcTypeParam;
