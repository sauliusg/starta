use std;

procedure print_array( int m[] )
{
    for var int i = 0 to last( m ) {
        < m[i]; < " ";
    }
    < "\n";
}

var b = new blob(200);

pack b, 0, "i4", [ [ 1, 2, 3 ], [ 4, 5, 6 ] ];

var m = unpack (array of int)( b, 0, "i4x10" );

print_array( m );
