#
# Snail compiler tests
#
#
# Test strto*() functions
#

use std;

var endidx: long;
var x : float;
var d : double;
var l : ldouble;

x, endidx = strtof( "3.414" );
. x, endidx;

d, endidx = strtod( "1.57" );
. d, endidx;

l, endidx = strtold( "0.71E8" );
. l, endidx;

x, endidx = strtof( "NaN" );
. x, endidx;

x, endidx = strtof( "Inf" );
. x, endidx;

d, endidx = strtod( "+Inf" );
. d, endidx;

l, endidx = strtold( "-Inf" );
. l, endidx;
