#
# Snail compiler tests
#
#
# Test function arument type compatibility
#

use std;

function g( x, y : int = 1; z, t : long; llong u = 10 ) : llong
{
    return x@llong + y@llong + (z + t)@llong + u;
}

. g( x=> 1, y => 1, 5, 7 ); // OK
. g( z => 5L, t=> 7, u => 10LL ); // OK
. g( z => 5L, 7, u => 10LL ); // OK
