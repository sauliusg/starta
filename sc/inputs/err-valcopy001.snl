#
# Snail compiler tests
#
#
# Test value-copy-assignment
#

use std;

var a : array of int = [ 1, 2, 3 ];
var b : array of float = [ 4.0, 5.0, 6.0 ];

a := b; // ERROR

.a[0];
.a[1];
.a[2];

var float m[][] = [ [1.0, 2.0], [3.0, 4.0] ];
var float n[][] = [ [0.0, 0.0], [0.0, 0.0] ];
var long  p[][] = [ [0L, 0L], [0L, 0L] ];

n[0] := m; // ERROR
n[0] := a; // ERROR
n[0] := b; // OK
n[0] := m[1]; // OK
n[0] := p[1]; // ERROR
