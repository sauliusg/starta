#
# Snail compiler tests
#
#
# Test synonim type compatibility.
#

use std;

type inches = float;
type cm = float;

var c : cm;
var i : inches;
var x : float;

i = 10; // ERROR
i = 10@float; // OK, float is compatible with its derived types.
i = 1.; // OK, constant 1. has type 'float' and is compatible with 'inches'.
i = c; // ERROR
c = i; // ERROR
c = x; // OK, base type can be assigned to the synonim
i = x; // OK, base type can be assigned to the synonim
x = i; // ERROR?
x = c; // ERROR?
