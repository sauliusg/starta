# Compiler tests.
#
# Test the '<filehandle>' file read condition.
#

from std use *;

var inp = fopen( "inputs/fileio011.snl", "r" );

var i = 1;
while( <inp> ) {
    . i, $_; i++
}

fclose( inp );

inp = fopen( "inputs/fileio011.snl", "r" );

i = 1;
while <inp> {
    . i, $_; i++
}

fclose( inp );
