from std use *;

procedure print_array( int m[] )
{
    for var int i = 0 to last( m ) {
        < m[i]; < " ";
    }
    < "\n";
}

var b = new blob(10);

pack b, 0, "I4", 0x01020304;

var m = unpack (array of int)( b, 0, "i1x4" );

print_array( m );

pack b, 0, "i4", 0x01020304;

m = unpack int[] ( b, 0, "i1x4" );

print_array( m );

. "0x%08X" %% unpack int( b, 0, "I4" );
. "0x%08X" %% unpack int( b, 0, "i4" );
