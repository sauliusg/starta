#
# Snail compiler tests
#
#
# Test readonlyness of function parameters.
#

from std use *;

function g( int i; array of int m ) : int
{
    if( i <= last( m )) {
        return i * m[i];
    } else {
        return 0;
    }
}

readonly a : array of int = [10,20,30,40,50,60,70,80];

. g( 7, a );

. a[0];
