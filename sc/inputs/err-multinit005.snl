#
# Snail language compiler tests
#
# Test multiple variable initialisation.
#

use std;

function multitype( int i ) : int, long, double
{
    return i * 10, i@long * 10000L, i@double/3.0D
}

// errors:

var x2, y2 : double = 1;

var x3, y3, z3 : double = 1, 3;

var x4, y4, z4, t4 = multitype( 4 );
