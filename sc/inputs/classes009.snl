#
# Test struct and class methods
#

from std use *;

type A = class {
     method name() : string
     begin
         return "class A"
     end;
}

class B {
     A a;
     method name() : string { return "class B" }
};

var b = new B;
b.a = new A;

. b->name();
. b.a->name();
