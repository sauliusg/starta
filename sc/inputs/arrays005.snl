#
# Snail compiler tests
#
#
# Testing cloning of arrays.
#

from std use *;

procedure print_array( int[][] m )
{
    < "[ ";
    for var i = 0 to last(m) do
        do < ", " if i > 0;
        < m[i][0]
    enddo
    < " ]\n";
}

var m : array [][] of int;
var n : array [][] of int = new int[3][3];

m = [ [10], [20], [30], [40] ];

n := m;

print_array( n );
