#
# Snail compiler tests
#

include "inputs/stdtypes.slib"

inline bytecode function write( i : int )
begin
    IPRINT
end

inline bytecode function writeln()
begin
    NEWLINE
end

var m : array [][] of int;

m = new int [10][20];

var i, j : int;

for i = 0 to 9 do
    for j = 0 to 19 do
        m[i][j] = i * j;
    enddo
enddo

for i = 0 to 9 do
    for j = 0 to 19 do
        write( m[i][j] );
        bytecode { SPACE }
    enddo
    writeln();
enddo
