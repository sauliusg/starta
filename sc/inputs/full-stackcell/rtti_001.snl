
use std;

var t: type of var;
var i: int;

bytecode function print_type_rtti( tt: type of var )
{
    RTTIDUMP
}

t = type int;
print_type_rtti( t );

t = type of i;
print_type_rtti( t );

t = type of t;
print_type_rtti( t );

type S = struct { name: string; m: array of int; i: int };

print_type_rtti( type string );
print_type_rtti( type S );
