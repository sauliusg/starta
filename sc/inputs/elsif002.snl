#
# Snail language compiler tests
#
#
# 'ELSIF' conditional operator test
#

from std use *;

var i : int;

i = -1;
while( i < 2 ) {
    . i;
    if i < 0 then
        . "i is NEGATIVE" 
    elsif i == 0 then
        . "i is ZERO"
    elsif i > 0 then
        . "i is POSITIVE"
    endif

    i ++;
}
