from std use *;

function f( float x; float y ) : float
{
    return x / y
}

function g( float x, y ) -> float
{
    return x / y
}

function h( x, y : float ) -> float
{
    return x / y
}

. f( 3, 2 );
. g( 3, 2 );
. h( 3, 2 );
