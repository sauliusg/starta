
use std;

function mini( point: type T;
               makearray: function( length : long ) -> (array of T);
               makecenter: function( simplex: array of T; i: long )->(type T)
             ) -> float, float
{
    return 0.0, 0.0;
}

function g( int x ): float
{
    return x + 2
}

. g( 5 );

function h( int x ): long, float
{
    return <long>x + 2L, x - 2
}

. h( 5 );

var x, y = h( 6 );

. x, y;
