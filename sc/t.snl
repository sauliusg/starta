// --*- C -*--

// Test derived types.

use std;

type synint = int;
type newint = new int;
type newint2 = new int;

var i, j, k : int;
var x, y, z : synint;
var a, b, c : newint;
var t, u, v : newint2;

x = i; // OK
j = y; // OK

i = a; // ERROR
a = i; // ERROR

x = 1; // OK
a = 2; // ERROR
a = 2 newint; // OK, explicit constant type

x = a; // ERROR
a = t; // ERROR

x = y + k; // OK
x = y + z; // OK

a = b + c; // OK
t = u + v; // OK

c = b + j; // ERROR
